# Interactive Brokers API Configuration
IBKR_HOST=127.0.0.1
IBKR_PORT=7497
IBKR_CLIENT_ID=1

# 7497 = Paper Trading (recommended for development)
# 7496 = Live Trading

# Market Data Type
# 1 = Live (requires market data subscription)
# 2 = Frozen (last available price when market closed)
# 3 = Delayed (15-20 min delay, free)
# 4 = Delayed Frozen
IBKR_MARKET_DATA_TYPE=2
